# ==================== 泛解析检测配置 ====================
# 泛解析检测测试数量
WILDCARD_TEST_COUNT=20

# 泛解析检测成功率阈值（百分比）
WILDCARD_SUCCESS_RATE_THRESHOLD=90

# 泛解析检测IP重复率阈值（百分比）
WILDCARD_IP_REPEAT_RATE_THRESHOLD=50

# ==================== 其他配置 ====================
# 通用子域名
COMMON_SUBNAMES=www,mail,ftp,admin,blog,api,dev,stage,prod,app,web,cdn,static,img,css,js,docs,help,support 